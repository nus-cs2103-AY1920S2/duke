<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.text.Text?>
<fx:root alignment="TOP_RIGHT" type="javafx.scene.layout.HBox" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"></Insets>
    </padding>
    <Text fx:id="dialog" textAlignment="JUSTIFY">
        <font>
            <Font name="Calibri Light" size="18.0"/>
        </font>
     </Text>
    <ImageView fx:id="displayPicture" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true"></ImageView>
</fx:root>
