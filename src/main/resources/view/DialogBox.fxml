<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="TOP_RIGHT" type="javafx.scene.layout.HBox" prefWidth="400" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <HBox>
      <children>
        <Label fx:id="dialog" text="Label" wrapText="true" style="-fx-font: 12 Monospace;" minWidth="100">
          <padding>
              <Insets left="10.0" right="10.0" top="5" bottom="5" />
          </padding>
        </Label>
      </children>
      <padding>
        <Insets left="10.0" right="10.0" top="10" bottom="10" />
      </padding>
    </HBox>
    <ImageView fx:id="displayPicture" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true"/>
  </children>
  <padding>
    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
  </padding>
</fx:root>