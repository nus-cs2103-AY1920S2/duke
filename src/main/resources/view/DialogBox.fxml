    <children>
      <BorderPane prefHeight="50.0" prefWidth="50.0" style="-fx-border-width: 1.0; -fx-border-color: c2cff2; -fx-padding: 10; -fx-border-radius: 20; -fx-background-radius: 20; -fx-background-color: #137F97" >
         <center>
              <Text fx:id="dialog" text="Text" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
        <ImageView fx:id="displayPicture" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
