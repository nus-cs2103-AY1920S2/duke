<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" pickOnBounds="false" style="-fx-border-color: black; -fx-border-width: 0px 0px 1px 0px;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" text="Label" wrapText="true" style="-fx-border-radius: 25px; -fx-background-color: white; -fx-padding: 20px;">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
        <ImageView fx:id="displayPicture" fitHeight="130.0" fitWidth="120.0" pickOnBounds="true" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
